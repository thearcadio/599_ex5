<header>
  <div class="jumbotron hidden-xs">
  </div><!-- jumbotron -->

  <nav class="navbar navbar-inverse">
    <div class="container">
      <div class="navbar-header">

        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>

        <a href="/" class="navbar-brand">My Favorite Places</a>
      </div>
      <div class="collapse navbar-collapse" id="navbar-collapse">
        <ul class="nav navbar-nav navbar-right">
          <li><a href="/">Home</a></li>
            <li><a href="/speakers" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Highlights <span class="caret"><span></a>
              <ul class="dropdown-menu">
                <li><a href="/speakers">All Highlights</a></li>
                <li role="separator" class="divider"></li>
                <% allSpeakers.forEach(function(item) { %>
                  <li><a href="/speakers/<%= item.id %>"><%= item.site %></a></li>
                <% }); %>
              </ul>
          </li>
          <li><a href="/speakers" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Categories <span class="caret"><span></a>
            <ul class="dropdown-menu">
              <li><a href="/speakers">All Categories</a></li>
              <li role="separator" class="divider"></li>
              <% var catTypes = [], i = 0, j = 0, inArray = false; %>
              <% allSpeakers.forEach(function(item) { %>
                <% for(i = 0; i < catTypes.length; i++){%>
                  <% if(item.category === catTypes[i]){ %>
                    <% inArray = true; %>
                  <% } %>
                <% } %>
                <% if(!inArray) {%>
                  <% catTypes[j] = item.category; %>
                  <% j++ %>
                <% } %>
                <% inArray = false;%>
              <% }); %>
              <% for(i = 0; i < catTypes.length; i++){%>
                <li><a href="/speakers/<%= catTypes[i]%>"><%= catTypes[i]%></a></li>
              <% } %>
            </ul>
        </li>
        </ul><!-- nav -->
      </div><!-- navbar-collapse -->
    </div><!-- container -->
  </nav>
</header>
